for (const name in plantsJson) {
  const { imgs }: any = plantsJson[name];
  console.log(name);
  if (fs.existsSync(`./plants/img/${name}`)) {
    const files = fs.readdirSync(`./plants/img/${name}`);

    plantsJson[name].imgs = files.map(
      (file: string) => `/plants/img/${name}/${file}`
    );
    console.log(name, plantsJson[name].imgs);
  }
}
fs.writeFileSync('plant2.json', JSON.stringify(plantsJson));
console.log('finish');
